#include "holberton.h"
/**
 * _memcpy - function that fills the fist n
 * @n: number of bytes of the momery area pointed to
 * @c: memory area
 * @s: string
 * Return: NULL
 */
char *_strchr(char *s, char c)
{
	int i;
	char *k;

	for (i = 0; s[i]; i++)
	{
		if (s[i] == c)
		{
			k = (s + i);
			break;
		}

	}
return ();
}
